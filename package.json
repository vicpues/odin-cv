{
  "name": "odin-cv",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "yarn ; yarn sdks vscode ; husky",
    "rename": "node -e \"const fs=require('fs'),path=require('path');const pkgPath=path.join(process.cwd(),'package.json');const lockPath=path.join(process.cwd(),'yarn.lock');const oldPkg=JSON.parse(fs.readFileSync(pkgPath));const oldName=oldPkg.name;const newName=path.basename(process.cwd());oldPkg.name=newName;fs.writeFileSync(pkgPath,JSON.stringify(oldPkg,null,2)+'\\n');if(fs.existsSync(lockPath)){let lock=fs.readFileSync(lockPath,'utf8');lock=lock.replace(new RegExp(oldName+'@workspace:\\.','g'),newName+'@workspace:.');fs.writeFileSync(lockPath,lock);}console.log('âœ… Renamed to',newName);\"",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "dist": "yarn predist ; git add -f dist && git commit dist -m \"Release\" && git subtree push --prefix dist origin gh-pages ; yarn postdist",
    "fmt": "yarn prettier ; yarn postcss",
    "lint": "yarn eslint ; yarn stylelint",
    "eslint": "eslint . --cache --cache-location ./.yarn/.cache/.eslintcache",
    "stylelint": "stylelint \"./src/**/*.css\" --cache --cache-location ./.yarn/.cache/.stylelintcache",
    "prettier": "prettier . --write --ignore-path .gitignore --cache --cache-location ./.yarn/.cache/.prettiercache | grep -v \"unchanged\"",
    "postcss": "postcss ./src/**/*.css --replace",
    "clean": "git rm -r --cached dist",
    "predist": "git push origin --delete gh-pages ; yarn build",
    "postdist": "yarn clean && git reset HEAD^"
  },
  "dependencies": {
    "@responsive-image/react": "^1.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.33.0",
    "@responsive-image/core": "^2.0.0",
    "@responsive-image/vite-plugin": "^2.0.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@yarnpkg/sdks": "^3.2.2",
    "corepack": "^0.34.0",
    "css-declaration-sorter": "^7.2.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.6.2",
    "stylelint": "^16.23.1",
    "vite": "^7.1.2",
    "vite-plugin-svgr": "^4.3.0"
  },
  "packageManager": "yarn@4.9.2",
  "prettier": {
    "tabWidth": 4,
    "overrides": [
      {
        "files": [
          "*.html",
          "*.css",
          "*.jsx",
          "*.json",
          "*.md"
        ],
        "options": {
          "tabWidth": 2
        }
      }
    ]
  },
  "postcss": {
    "map": false,
    "plugins": {
      "css-declaration-sorter": {
        "order": "concentric-css"
      }
    }
  }
}
